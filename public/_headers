  # One year
  Cache-Control: max-age=31536000, public
/*
  Accept-Ranges: bytes
  Timing-Allow-Origin: *
  Server: Yuishinkai Karate Server
  X-Frame-Options: SAMEORIGIN
  X-XSS-Protection: 1; mode=block

/offline-service-worker.js
